@page
@model PrivacyModel
@{
    ViewData["Title"] = "Chart";
}
<h1>@ViewData["Title"]</h1>
<p class="display-6">Select a Trade Code from the table to check the OHLC Graph</p>

<div id="parent" class="container-fluid row">
  <div class="col-4">
    <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
              <th>Trade Code</th>
              <th>Check</th>
            </tr>
        </thead>
    </table>
  </div>

  <div id="chartContainer" class="col-8">
    <div id="chartContainer1" class="col-12 row"></div>
    <div id="chartContainer2" class="col-12 row" style="margin-top: 350px;"></div>
  </div>
</div>
@section Scripts{ 
    <script src="~/js/chart.js"></script>
}